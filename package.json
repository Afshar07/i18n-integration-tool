{
  "name": "i18n-integration-tool",
  "version": "1.0.0",
  "description": "Automated i18n integration tool for Nuxt.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "i18n-integrate": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "nuxt",
    "vue",
    "automation",
    "persian",
    "arabic",
    "translation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/i18n-integration-tool.git"
  },
  "homepage": "https://github.com/your-username/i18n-integration-tool#readme",
  "bugs": {
    "url": "https://github.com/your-username/i18n-integration-tool/issues"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@vue/compiler-sfc": "^3.3.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "glob": "^10.3.0"
  }
}
